// --- simple list declaration & output using “ord” ---
ord declare fruits = ["apple", "banana", "cherry"];
output fruits;         // 👉 prints: ['apple', 'banana', 'cherry']

// --- same with the “order” keyword ---
order declare letters = ["a", "b", "c"];
output letters;        // 👉 prints: ['a', 'b', 'c']

// --- empty list ---
ord declare empty = [];
output empty;          // 👉 prints: []

// --- numeric lists ---
ord declare numbers = [1, 2, 3, 4, 5];
output numbers;        // 👉 prints: [1, 2, 3, 4, 5]

// --- mixed-type lists (strings, ints, bools) ---
ord declare mixed = ["red", 7, true];
output mixed;          // 👉 prints: ['red', 7, true]